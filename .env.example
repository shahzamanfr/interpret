# AI Communication Coach - Environment Variables
# Copy this file to .env and fill in your API keys

# REQUIRED: Groq API Keys (for coaching feedback)
# You can add up to 4 keys for automatic quota multiplier
# When one key hits quota limit, system automatically tries the next one
VITE_GROQ_API_KEY=your_first_groq_key_here
VITE_GROQ_API_KEY_2=your_second_groq_key_here
VITE_GROQ_API_KEY_3=your_third_groq_key_here
VITE_GROQ_API_KEY_4=your_fourth_groq_key_here

# OPTIONAL: Pexels API Key (fallback images used if not provided)
VITE_PEXELS_API_KEY=your_pexels_api_key_here

# OPTIONAL: Unsplash API Key (fallback images used if not provided)
VITE_UNSPLASH_ACCESS_KEY=your_unsplash_api_key_here

# OPTIONAL: OCR.space API Key (for text extraction from files)
VITE_OCR_SPACE_API_KEY=your_ocr_key_here

# Backend Configuration (if using backend)
SPEECH_API_KEY=your_speech_api_key_here
SPEECH_PROVIDER=assemblyai
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000
PORT=8787
NODE_ENV=development
